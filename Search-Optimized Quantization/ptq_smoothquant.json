{
    "input_model": {
        "type": "PyTorchModel",
        "config": {
            "model_loader": "load_optimized_model",
            "model_script": "user_script.py",
            "io_config": {
                "input_names": ["input_ids", "attention_mask", "token_type_ids"],
                "input_shapes": [[1, 128], [1, 128], [1, 128]],
                "input_types": ["int64", "int64", "int64"],
                "output_names": ["output"]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "mse",
                    "type": "mse",
                    "sub_types": [
                        {
                            "name": "mse",
                            "priority": 1,
                            "goal": {
                                "type": "min-improvement",
                                "value": 0.01
                            }
                        }
                    ]
                },
                {
                    "name": "pearson",
                    "type": "pearson",
                    "sub_types": [
                        {
                            "name": "pearson",
                            "priority": 1,
                            "goal": {
                                "type": "max-improvement",
                                "value": 0.01
                            }
                        }
                    ]
                },
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "sub_types": [
                        {
                            "name": "accuracy_score",
                            "priority": 1,
                            "goal": {
                                "type": "percent-max-degradation",
                                "value": 2
                            }
                        }
                    ],
                    "user_config": {
                        "post_processing_func": "post_process",
                        "user_script": "user_script.py",
                        "dataloader_func": "create_dataloader",
                        "batch_size": 1
                    }
                },
                {
                    "name": "precision",
                    "type": "precision",
                    "data_config": "medsts_dataset"
                },
                {
                    "name": "recall",
                    "type": "recall",
                    "data_config": "medsts_dataset"
                },
                {
                    "name": "f1",
                    "type": "f1",
                    "data_config": "medsts_dataset"
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [
                        {
                            "name": "avg",
                            "priority": 2,
                            "goal": {
                                "type": "percent-min-improvement",
                                "value": 20
                            }
                        }
                    ],
                    "user_config": {
                        "user_script": "user_script.py",
                        "dataloader_func": "create_dataloader",
                        "batch_size": 1
                    }
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 14
            }
        },
        "transformers_optimization": {
            "type": "OrtTransformersOptimization",
            "config": {
                "model_type": "bert"
            }
        },
        "quantization": {
            "type": "IncDynamicQuantization",
            "config": {
                "quant_format": "QOperator",
                "user_script": "user_script.py",
                "dataloader_func": "inc_medsts_calibration_reader",
                "recipes": {
                    "smooth_quant": true,
                    "smooth_quant_args": {
                        "alpha": 0.5
                    }
                },
                "metrics": [
                    {
                        "name": "accuracy",
                        "type": "custom",
                        "sub_types": [
                            {
                                "name": "accuracy_custom",
                                "priority": 1,
                                "higher_is_better": true,
                                "goal": {
                                    "type": "percent-max-degradation",
                                    "value": 0.01
                                }
                            }
                        ],
                        "user_config": {
                            "user_script": "user_script.py",
                            "evaluate_func": "eval_accuracy",
                            "batch_size": 1
                        }
                    },
                    {
                        "name": "precision",
                        "type": "precision",
                        "user_config": {
                            "user_script": "user_script.py",
                            "evaluate_func": "eval_accuracy",
                            "batch_size": 1
                        }
                    },
                    {
                        "name": "recall",
                        "type": "recall",
                        "user_config": {
                            "user_script": "user_script.py",
                            "evaluate_func": "eval_accuracy",
                            "batch_size": 1
                        }
                    },
                    {
                        "name": "f1",
                        "type": "f1",
                        "user_config": {
                            "user_script": "user_script.py",
                            "evaluate_func": "eval_accuracy",
                            "batch_size": 1
                        }
                    },
                    {
                        "name": "latency",
                        "type": "latency",
                        "sub_types": [
                            {
                                "name": "avg",
                                "priority": 2,
                                "goal": {
                                    "type": "percent-min-improvement",
                                    "value": 20
                                }
                            }
                        ],
                        "user_config": {
                            "user_script": "user_script.py",
                            "evaluate_func": "eval_accuracy",
                            "batch_size": 1
                        }
                    }
                ]
            }
        }
    },
    "search_strategy": {
        "execution_order": "joint",
        "search_algorithm": "exhaustive"
    },
    "engine": {
        "evaluator": "common_evaluator",
        "cache_dir": "cache",
        "output_dir": "models/quantized_model"
    }
}